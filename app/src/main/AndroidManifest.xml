<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zazu.kaeff.net.zazu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="net.kaeff.zazu.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.kaeff.zazu.LogTimeActivity"
            android:label="@string/title_activity_log_time"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="net.kaeff.zazu.ShowTimeLogsActivity"
            android:label="@string/title_activity_show_time_logs"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
